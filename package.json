{
  "name": "@cc/express",
  "version": "1.0.0",
  "description": "expressjs-boilerplate",
  "engines": {
    "node": ">=16.x"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test jest",
    "dev": "nodemon --exec babel-node server.js",
    "start": "node dist/server.js",
    "prebuild": "rimraf dist",
    "build": "npx babel src --out-dir dist"
  },
  "repository": "git@github.com:codecontinent/expressjs.git",
  "author": "Mahabub Alam <mahabubx7@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codecontinent/expressjs/issues"
  },
  "homepage": "https://github.com/codecontinent/expressjs",
  "dependencies": {
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-hbs": "^2.4.0",
    "hbs": "^4.2.0",
    "mongoose": "^7.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/node": "^7.20.7",
    "@babel/preset-env": "^7.21.5",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.2",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "rimraff": "^0.0.1-security",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.(js,cjs,mjs,json)": "yarn eslint .",
    "*.(js,json,hbs,html,cjs,mjs)": "yarn prettier . -w"
  },
  "keywords": [
    "express",
    "nodejs",
    "rest-api"
  ]
}
